      "attorney_representation_allowed": "Boolean - whether attorneys can make requests"
    },
    "request_requirements": {
      "written_request_required": "Boolean - whether request must be in writing",
      "specific_format_required": "Boolean - whether specific form/format mandated",
      "purpose_statement_required": "Boolean - whether purpose must be stated",
      "identification_required": "Boolean - whether requestor ID required",
      "notarization_required": "Boolean - whether notarization needed",
      "preferred_delivery_method": "Array - agency preferred delivery methods",
      "acceptable_submission_methods": "Array - valid ways to submit requests",
      "required_information": "Array - mandatory elements for valid request"
    },
    "response_timelines": {
      "initial_response_days": "Number - days for initial acknowledgment",
      "final_response_days": "Number - days for complete response",
