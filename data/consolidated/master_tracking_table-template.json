      "weekend_holidays_counted": "Boolean - whether weekends/holidays count",
      "business_days_only": "Boolean - whether timeline uses business days only"
    },
    "fee_structure": {
      "search_fees_allowed": "Boolean - whether search time can be charged",
      "copying_fees_allowed": "Boolean - whether copying costs can be charged",
      "per_page_cost": "Number - cost per page for copies",
      "hourly_rate": "Number/null - hourly rate for search/review",
      "minimum_fee": "Number/null - minimum fee before waiver",
      "fee_waiver_available": "Boolean - whether fee waivers available",
      "fee_waiver_criteria": "Array - grounds for fee waiver",
      "advance_payment_threshold": "Number - amount requiring advance payment"
    },
    "appeal_process": {
      "administrative_appeal_available": "Boolean - whether internal appeal exists",
      "appeal_deadline_days": "Number - days to file appeal",
      "appeal_authority": "String - who reviews appeals",
      "judicial_review_available": "Boolean - whether court review available",
      "judicial_review_deadline_days": "Number - days to file court action",
      "attorney_fees_recoverable": "Boolean - whether prevailing party gets fees",
      "appeal_fee_required": "Boolean - whether appeal requires fee"
    },
    "unique_features": {
      "attorney_general_opinion_process": "Boolean - whether AG issues binding opinions",
      "mandatory_disclosure_categories": "Array - categories that must be disclosed",
      "proactive_disclosure_requirements": "Array - records that must be posted online",
      "expedited_processing_available": "Boolean - whether urgent requests expedited",
      "privacy_redaction_standards": "String - approach to privacy redactions",
      "electronic_records_provisions": "Boolean - whether law covers electronic records",
      "social_media_coverage": "Boolean - whether social media records covered"
    },
    "enforcement_mechanisms": {
      "criminal_penalties": "Boolean - whether criminal penalties exist for violations",
      "civil_penalties": "Boolean - whether civil fines available",
      "disciplinary_action": "Boolean - whether employee discipline required",
      "mandamus_available": "Boolean - whether writ of mandamus available",
      "attorney_fees_available": "Boolean - whether attorney fees recoverable",
      "damages_available": "Boolean - whether monetary damages available"
    },
