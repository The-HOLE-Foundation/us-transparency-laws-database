{
  "name": "export-transparency",
  "description": "Export complete 51-jurisdiction transparency database to various formats",
  "command": "python scripts/simple_transparency_export.py",
  "parameters": [
    {
      "name": "format",
      "type": "choice",
      "choices": ["supabase", "api", "csv", "markdown", "summary", "all"],
      "default": "summary",
      "description": "Export format"
    }
  ],
  "examples": [
    {
      "command": "/export-transparency supabase",
      "description": "Generate SQL for Supabase import"
    },
    {
      "command": "/export-transparency api",
      "description": "Create API-ready JSON format"
    },
    {
      "command": "/export-transparency all",
      "description": "Generate all export formats"
    }
  ],
  "working_directory": "foia-generator-setup",
  "output_directory": "exports/"
}